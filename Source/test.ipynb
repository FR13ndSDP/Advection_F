    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "a = np.matrix([[1,2],[3,4]])\n",
    "inv_a = np.linalg.inv(a)\n",
    "x = np.linspace(0,2*np.pi)\n",
    "y = np.sin(x)\n",
    "plt.plot(x,y)\n",
    "print(\"inverse of matrix a: \\n\", inv_a)"
   ]
  }
